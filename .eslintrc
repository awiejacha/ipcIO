{
  "extends": ["standard"],
  "rules": {
    "quotes": 0,
    "comma-dangle": [1,"always-multiline"],
    "no-cond-assign": [1,"except-parens"],
    "no-constant-condition": 1,
    "no-control-regex": 1,
    "no-debugger": 1,
    "no-dupe-args": 1,
    "strict": [1,"global"],
    "semi": 0,
    "camelcase": 0,
    "space-before-function-paren": 0,
    "brace-style": 0,
    "padded-blocks": 0
  }
}
